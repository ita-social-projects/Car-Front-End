{
    "name": "Car",
    "version": "0.0.1",
    "private": true,
    "rnpm": {
        "assets": [
            "./assets/fonts/"
        ]
    },
    "babel": {
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ],
            [
                "@babel/plugin-proposal-class-properties",
                {
                    "loose": true
                }
            ]
        ]
    },
    "scripts": {
        "android": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,tsx,json} && react-native run-android",
        "ios": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,tsx,json} && react-native run-ios",
        "start": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,tsx,json} && react-native start",
        "test": "jest --coverage",
        "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
        "prettier:write": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,tsx,json}"
    },
    "dependencies": {
        "@babel/core": "^7.13.1",
        "@babel/plugin-proposal-decorators": "^7.13.5",
        "@microsoft/microsoft-graph-client": "^2.2.1",
        "@microsoft/signalr": "^5.0.3",
        "@react-native-community/async-storage": "^1.12.1",
        "@react-navigation/bottom-tabs": "^5.11.8",
        "@react-navigation/native": "^5.9.3",
        "@react-navigation/stack": "^5.14.3",
        "axios": "^0.21.1",
        "babel-plugin-transform-typescript-metadata": "^0.3.1",
        "metro-react-native-babel-preset": "^0.65.1",
        "moment": "^2.29.1",
        "react": "^16.13.1",
        "react-hook-form": "^6.15.4",
        "react-native": "^0.63.4",
        "react-native-app-auth": "^6.1.0",
        "react-native-dropdown-picker": "^4.0.2",
        "react-native-elements": "^3.2.0",
        "react-native-extended-stylesheet": "^0.12.0",
        "react-native-image-picker": "^3.2.1",
        "react-native-keyboard-aware-scroll-view": "^0.9.3",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-restart": "^0.0.20",
        "react-native-text-gradient": "^0.1.7",
        "react-native-vector-icons": "^8.1.0",
        "reanimated-bottom-sheet": "^1.0.0-alpha.22",
        "string-to-color": "^2.2.2"
    },
    "jest": {
        "preset": "react-native"
    }
}
