{
    "name": "Car",
    "version": "0.0.1",
    "private": true,
    "rnpm": {
        "assets": [
            "./assets/fonts/"
        ]
    },
    "babel": {
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ]
        ]
    },
    "scripts": {
        "android": "tsc --noEmit && eslint --fix --ext .js,.jsx,.ts,.tsx ./ && react-native run-android",
