        "@types/react-native-vector-icons": "6.4.6",
        "cross-env": "^7.0.3",
        "eslint": "^6.8.0",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "eslint-plugin-unused-imports": "^1.1.0",
        "husky": "^5.1.3",
        "jest": "^25.1.0",
        "react-native-cli": "^2.0.1",
        "react-native-linear-gradient": "^2.5.6",
