{
    "name": "Car",
    "version": "0.0.1",
    "private": true,
    "rnpm": {
        "assets": [
            "./assets/fonts/"
        ]
    },
    "babel": {
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ],
            [
                "@babel/plugin-proposal-class-properties",
                {
                    "loose": true
                }
            ]
        ]
    },
    "scripts": {
        "android": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,tsx,json} && react-native run-android",
        "ios": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,tsx,json} && react-native run-ios",
        "start": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,tsx,json} && react-native start",
        "test": "jest --coverage",
        "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
        "prettier:write": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,tsx,json}"
    },
    "dependencies": {
        "@babel/plugin-proposal-decorators": "7.12.1",
        "@microsoft/microsoft-graph-client": "2.1.1",
        "@microsoft/signalr": "5.0.1",
        "@react-native-community/async-storage": "^1.12.1",
        "@react-native-community/masked-view": "0.1.10",
        "@react-navigation/bottom-tabs": "5.11.2",
        "@react-navigation/native": "5.8.10",
        "@react-navigation/stack": "5.12.8",
        "axios": "0.21.1",
        "fbjs": "^3.0.0",
        "isomorphic-fetch": "3.0.0",
        "moment": "^2.29.1",
        "npm": "^7.5.4",
        "react": "16.13.1",
        "react-hook-form": "6.14.2",
        "react-native": "^0.63.4",
        "react-native-app-auth": "^6.0.1",
        "react-native-dropdown-picker": "3.7.6",
        "react-native-elements": "3.2.0",
        "react-native-extended-stylesheet": "0.12.0",
        "react-native-gesture-handler": "1.9.0",
        "react-native-image-picker": "3.1.2",
        "react-native-keyboard-aware-scroll-view": "0.9.3",
        "react-native-reanimated": "1.13.2",
        "react-native-rename": "^2.7.0",
        "react-native-restart": "^0.0.20",
        "react-native-safe-area-context": "3.1.9",
        "react-native-screens": "2.15.0",
        "react-native-text-gradient": "0.1.7",
        "react-native-vector-icons": "7.1.0",
        "reanimated-bottom-sheet": "1.0.0-alpha.22",
        "string-to-color": "^2.2.2"
    },
    "devDependencies": {
        "@babel/core": "7.8.4",
        "@babel/runtime": "7.8.4",
        "@react-native-community/eslint-config": "1.1.0",
        "@types/react": "17.0.0",
        "@types/react-native": "0.63.37",
        "@types/react-native-vector-icons": "6.4.6",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "eslint": "^7.19.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.19.0",
        "jest": "25.1.0",
        "prettier": "^2.2.1",
        "react-native-cli": "^2.0.1",
        "react-native-linear-gradient": "^2.5.6",
        "react-test-renderer": "16.13.1",
        "typescript": "4.1.2"
    },
    "jest": {
        "preset": "react-native"
    }
}
