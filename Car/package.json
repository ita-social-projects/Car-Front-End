        "start": "tsc --noEmit && eslint --fix --ext .js,.jsx,.ts,.tsx ./ && react-native start",
        "test": "jest --coverage",
        "lint": "tsc --noEmit && eslint --fix --ext .js,.jsx,.ts,.tsx ./ && jest --coverage",
        "postinstall": "cd .. && husky install || husky install || 0"
    },
    "dependencies": {
